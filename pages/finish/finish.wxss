/* pages/finish/finish.wxss */
.box_finish{
  width: 100%;
  height: 100%;
  padding: 0 40rpx;
  box-sizing: border-box;
}
.fenxiang{
  font-size: 28rpx;
  color: #C2C2C2;
  margin-top: 37rpx;
}
.shengc{
  font-size: 38rpx;
  color: #3F3F3F;
  margin-top: 10rpx;
}
image{
  width: 672rpx;
  height: 818rpx;
  object-fit: cover;
  margin-top: 37rpx;
}
.bott_fi{
  width: 442rpx;
  height: 68rpx;
  margin: 53rpx auto 0;
  display: flex;
  align-items: center;
  justify-content: space-between;
}
.bott_fi button{
  width: 206rpx;
  height: 100%;
  border-radius: 38rpx;
  text-align: center;
  line-height: 68rpx;
  font-size: 28rpx;
  padding: 0;
}
.bott_fi button:nth-of-type(1){
  border: 1px solid #3D3D3D;
  background: none;
  color: #3D3D3D;
}
.bott_fi button:nth-of-type(2){
  background: #3D3D3D;
  color: #fff;
}
